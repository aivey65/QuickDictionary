{
  "manifest_version": 3,
  "name": "Quick Dictionary",
  "version": "1.0.0",
  "author": "Asha Ivey",
  "description": "Quickly view the definition of a word when highlighted.",
  "icons": {
    "16": "images/quick-dictionary_16.png",
    "32": "images/quick-dictionary_32.png",
    "48": "images/quick-dictionary_48.png",
    "128": "images/quick-dictionary_128.png"
  },
  "action":{
    "default_popup": "index.html",
    "default_title": "Quick Dictionary"
  }
}